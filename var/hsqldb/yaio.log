INSERT INTO BASE_NODE VALUES('InfoNode','DT2015051323373611093',NULL,'weiter','','Wie Weiter','DIV',4,1209,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,'17','SysRoad','','','Wie wird es weitergehen...','Ja, Ja D''Artagnan ist noch lange (und wohl niemals ;-) fertig. St\u00e4ndig fallen mir weitere kleine Features ein, die ich noch unterbringen m\u00f6chte.<WLBR>Deshalb hioer eine kleine \u00dcbersicht, was die auf lange Sicht geplant und was davon auch schon umgesetzt wurde.',NULL,NULL,NULL,NULL,NULL,NULL,NULL,0,'INFO - Roadmap                                                                                                                                                  [NodeMeta: SysRoad,1,,] [NodeSys: SysRoad1,13.05.2015 23:37,BB307FF57F1AA571653841B86FD7CD70,13.05.2015 23:37,1]\u0009INFO - Wie wird es weitergehen...                                               [DocLayout: DIV,weiter,Wie Weiter,]                                             [NodeMeta: SysRoad,17,,] [NodeSys: DT2015051323373611093,13.05.2015 23:37,80000DEB74497DF902AD4C3AFF970312,13.05.2015 23:37,1] ProjektDesc:Ja, Ja D''Artagnan ist noch lange (und wohl niemals ;-) fertig. St\u00e4ndig fallen mir weitere kleine Features ein, die ich noch unterbringen m\u00f6chte.<WLBR>Deshalb hioer eine kleine \u00dcbersicht, was die auf lange Sicht geplant und was davon auch schon umgesetzt wurde.','INFO',2,'2015-05-13 23:37:36.246000','2015-05-13 23:37:00.000000','0AA11982391D9849F0F483EE5FFCF795','INFO',0,NULL,NULL,NULL,NULL,NULL,NULL,'SysRoad1',NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,NULL,7,0,0,0,NULL)
