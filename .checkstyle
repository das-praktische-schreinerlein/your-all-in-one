<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="MS" location="checkstyle.xml" type="project" description="">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="MS" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
  <filter name="DerivedFiles" enabled="true"/>
  <filter name="FilesFromPackage" enabled="true">
    <filter-data value="src/main/resources/META-INF"/>
    <filter-data value="src/main/resources/static/css/highlightjs"/>
    <filter-data value="src/main/resources/static/css/jqueryui"/>
    <filter-data value="src/main/resources/static/css/slimbox2"/>
    <filter-data value="src/main/resources/static/images"/>
    <filter-data value="src/main/resources/static/js"/>
  </filter>
</fileset-config>
